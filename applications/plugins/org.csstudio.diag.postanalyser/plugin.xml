<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.csstudio"
            name="CSS"/>
      <view
            category="org.csstudio"
            class="org.csstudio.diag.postanalyser.View"
            icon="icons/mainLabel.png"
            id="org.csstudio.diag.postanalyser.view"
            name="%PostAnalyzer">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            id="org.csstudio.diag.postanalyser.object_contrib"
            objectClass="org.csstudio.platform.model.IProcessVariableWithSamples">
         <menu
               id="org.csstudio.popmenu"
               label="CSS"
               path="additions">
            <groupMarker name="content"/>
            <separator name="additions"/>
         </menu>
         <action
               class="org.csstudio.diag.postanalyser.ObjectContribPopupAction"
               icon="icons/mainLabel.png"
               id="org.csstudio.diag.postanalyser.action1"
               label="%PostAnalyzer"
               menubarPath="org.csstudio.popmenu/content"/>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.csstudio.diag.postanalyser.ShowAnalyserAction"
            label="CSS Action Set"
            visible="true">
         <action
               class="org.csstudio.diag.postanalyser.ShowAnalyserAction"
               id="org.csstudio.diag.postanalyser.showAnalyser"
               icon="icons/mainLabel.png"
               label="%PostAnalyzer"
               menubarPath="css/diag/"
               style="push"/>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true"/>
   </extension>

</plugin>
