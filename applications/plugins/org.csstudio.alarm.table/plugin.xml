<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="CSS"
            id="org.csstudio">
      </category>
      <view
            category="org.csstudio"
            class="org.csstudio.alarm.table.LogViewArchive"
            icon="icons/archiveTable.png"
            id="org.csstudio.alarm.table.LogViewArchive"
            name="JMS Log archive viewer"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="CSS"
            id="org.csstudio">
      </category>
      <view
            category="org.csstudio"
            class="org.csstudio.alarm.table.LogView"
            icon="icons/logTable.png"
            id="org.csstudio.alarm.table.LogView"
            name="JMS Log viewer"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="CSS"
            id="org.csstudio">
      </category>
      <view
            category="org.csstudio"
            class="org.csstudio.alarm.table.AlarmLogView"
            icon="icons/alarmTable.png"
            id="org.csstudio.alarm.table.AlarmLogView"
            name="JMS Alarm viewer"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="CSS"
            id="org.csstudio">
      </category>
      <view
            category="org.csstudio"
            class="org.csstudio.alarm.table.PropertiesView"
            icon="icons/logTable.png"
            id="org.csstudio.alarm.table.PropertiesView"
            name="JMS properties viewer"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.desy.css.jmsLogs.page3"
            class="org.csstudio.alarm.table.preferences.LogViewerPreferencePage"
            id="de.desy.css.jmsLogs.preferences.JmsLogsPreferencePage"
            name="Jms Log viewer"/>
      <page
            category="de.desy.css.jmsLogs.page3"
            class="org.csstudio.alarm.table.preferences.AlarmViewerPreferencePage"
            id="de.desy.css.jmsLogs.preferences.AlarmViewerPreferencePage"
            name="JMS Alarm viewer"/>
      <page
            category="de.desy.css.jmsLogs.page3"
            class="org.csstudio.alarm.table.preferences.LogArchiveViewerPreferencePage"
            id="de.desy.css.jmsLogs.preferences.LogArchiveViewerPreferencePage"
            name="JMS Log archive viewer"/>
      <page
            category="org.csstudio.platform.ui.css.applications.alarm"
            class="org.csstudio.alarm.table.preferences.JmsLogPreferencePage"
            id="de.desy.css.jmsLogs.page3"
            name="JMS Logs"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.csstudio.alarm.table.preferences.LogViewerPreferenceInitializer"/>
      <initializer class="org.csstudio.alarm.table.preferences.LogArchiveViewerPreferenceInitializer"/>
      <initializer class="org.csstudio.alarm.table.preferences.JmsLogPreferenceInitializer"/>
      <initializer class="org.csstudio.alarm.table.preferences.AlarmViewerPreferenceInitializer"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Global CSS menu actions"
            id="de.desy.css.jmsLogs"
            label="CSS Action Set"
            visible="true">
         <action
               class="org.csstudio.alarm.table.ShowLogArchive"
               icon="icons/archiveTable.png"
               id="de.desy.css.jmsLogs.ShowViewArchiveAction"
               label="JMSLogsArchive"
               menubarPath="css/alarm/"
               style="push"
               tooltip="Show JMS Log archive"/>
         <action
               class="org.csstudio.alarm.table.ShowLog"
               icon="icons/logTable.png"
               id="de.desy.css.jmsLogs.ShowViewLogAction"
               label="JMSLogs"
               menubarPath="css/diag/"
               style="push"
               tooltip="Show JMS Log"/>
         <action
               class="org.csstudio.alarm.table.ShowAlarms"
               icon="icons/alarmTable.png"
               id="de.desy.css.jmsLogs.ShowViewAlarmAction"
               label="JMSAlarmLog"
               menubarPath="css/alarm/"
               style="push"
               tooltip="Show JMS Alarms"/>

      </actionSet>
   </extension>
   <extension
         point="org.csstudio.platform.controlSystemItemFactories">
      <controlSystemItemFactory
            class="org.csstudio.alarm.table.dataModel.TextContainerFactory"
            typeId="org.csstudio.alarm.table.controlSystemItemFactory1"/>
   </extension>
      <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            id="org.csstudio.alarm.table.popupMenu"
            objectClass="org.csstudio.platform.model.IProcessVariable">
         <menu
               id="org.csstudio.popmenu"
               label="CSS"
               path="additions">
            <groupMarker name="content"/>
            <separator name="additions"/>
         </menu>
         <action
               class="org.csstudio.alarm.table.ArchivePopupAction"
               enablesFor="1"
               icon="icons/archiveTable.png"
               id="org.csstudio.alarm.table.archivepopupAction"
               label="Log Archive"
               menubarPath="org.csstudio.popmenu/content"/>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.csstudio.alarm.table.dataModel.JMSMessage"
            class="org.csstudio.alarm.table.dataModel.MessagePropertySourceAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
   </extension>

   

</plugin>
