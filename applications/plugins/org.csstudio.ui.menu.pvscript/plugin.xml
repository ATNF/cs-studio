<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.csstudio.ui.menu.popup.processvariable">
         <dynamic
               class="org.csstudio.ui.menu.pvscript.ContextMenuItems"
               id="org.csstudio.ui.menu.pvscript.items">
         </dynamic>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.csstudio.ui.menu.pvscript.RunScriptHandler"
            id="org.csstudio.ui.menu.pvscript.runscript"
            name="RunScript">
         <commandParameter
               id="script"
               name="Script"
               optional="false">
         </commandParameter>
      </command>
   </extension>

</plugin>
