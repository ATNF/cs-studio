<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.csstudio.utility.quickstart.menu">
         <dynamic
               class="org.csstudio.utility.quickstart.MyCompoundContributionItem1"
               id="org.csstudio.utility.quickstart.myCompoundContributionItem1">
         </dynamic>
         <dynamic
               class="org.csstudio.utility.quickstart.MyCompoundContributionItem2"
               id="org.csstudio.utility.quickstart.myCompoundContributionItem2">
         </dynamic>
      </menuContribution>
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=CSS">
         <menu
               id="org.csstudio.utility.quickstart.menu"
               label="Quickstart">
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.csstudio.utility.quickstart.quickstartCategory"
            name="QuickstartCategory">
      </category>
      <command
            categoryId="org.csstudio.utility.quickstart.quickstartCategory"
            id="org.csstudio.utility.quickstart.command1"
            name="Command1">
      </command>
      <command
            categoryId="org.csstudio.utility.quickstart.quickstartCategory"
            id="org.csstudio.utility.quickstart.command2"
            name="Command2">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.csstudio.utility.quickstart.CommandHandler1"
            commandId="org.csstudio.utility.quickstart.command1">
      </handler>
      <handler
            class="org.csstudio.utility.quickstart.CommandHandler2"
            commandId="org.csstudio.utility.quickstart.command2">
      </handler>
   </extension>
     <extension
        point="org.eclipse.ui.popupMenus">
     <objectContribution
           adaptable="false"
           id="org.csstudio.utility.quickstart.addtoquickstart"
           nameFilter="*.css-sds"
           objectClass="org.eclipse.core.resources.IResource">
        <action
              class="org.csstudio.utility.quickstart.AddToQuickstartAction"
              id="org.csstudio.utility.quickstart.addtoquickstartaction"
              label="Add to quickstart"/>
     </objectContribution>
  </extension>
     <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.csstudio.platform.ui.css.applications."
            class="org.csstudio.utility.quickstart.preferences.QuickstartPreferencePage"
            id="org.csstudio.utility.quickstart.preferences.QuickstartPreferencePage"
            name="Quickstart"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.csstudio.utility.quickstart.preferences.PreferenceInitializer"/>
   </extension>
  

</plugin>
