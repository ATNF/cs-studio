# Test database
#
#   softIoc -s -d test.db

record(calc, "fred")
{
   field(SCAN, "1 second")
   field(CALC, "RNDM")
   field(EGU,  "furlong")
   field(PREC, "4")
}

record(calc, "janet")
{
   field(SCAN, ".1 second")
   field(CALC, "10+5*RNDM")
   field(EGU,  "furlong")
   field(PREC, "4")
   field(FLNK, "hist")
}

record(compress, "hist")
{
   field(INP,  "janet")
   field(NSAM, "50")
   field(ALG,  "Circular Buffer")
}

record(waveform, "longs")
{
   field(INP,  "hist")
   field(SCAN, "1 second")
   field(NELM, "50")
   field(FTVL, "LONG")
}