<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="pvutildata" name="PVUtilData" schema="schema/pvutildata.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.csstudio"
            name="CSS">
      </category>
      <view
            id="org.csstudio.diag.pvutil.view.PVUtilView"
            category="org.csstudio"
            class="org.csstudio.diag.pvutil.view.PVUtilView"
            icon="icons/PVU.JPG"
            name="PV Utility">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Global CSS menu actions"
            id="org.csstudio.pvutil"
            label="CSS Action Set"
            visible="true">
         <action
               class="org.csstudio.diag.pvutil.view.NewPVUtilityAction"
               icon="icons/PVU.JPG"
               id="org.csstudio.pvutil.view.NewPVUtilityAction"
               label="PV Utility"
               menubarPath="css/diag/"
               style="push">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.csstudio.pvutil.OpenViewWithPVPopupAction"
            objectClass="org.csstudio.platform.model.IProcessVariable">
         <menu
               id="org.csstudio.pvutil.popmenu"
               label="CSS"
               path="additions">
         </menu>
         <action
               class="org.csstudio.diag.pvutil.view.PVpopupAction"
               enablesFor="1"
               icon="icons/PVU.JPG"
               id="org.csstudio.pvutil.view.PVpopupAction"
               label="PV Utility"
               menubarPath="org.csstudio.popmenu/content"
               tooltip="%PopupMenuTooltip">
         </action>
      </objectContribution>
   </extension>

</plugin>
