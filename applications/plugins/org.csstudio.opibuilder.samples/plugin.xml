<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.csstudio.opibuilder.samples.sampleset" name="Sample Set" schema="schema/org.csstudio.opibuilder.samples.sampleset.exsd"/>
   <extension
         point="org.csstudio.opibuilder.samples.sampleset">
      <sampleset
            name="basic"
            sampleset="org.csstudio.opibuilder.samples.Basic">
      </sampleset>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.csstudio.opibuilder.samples.Install"
            id="org.csstudio.opibuilder.samples.install"
            name="Install Boy Examples">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:utility">
         <command
               commandId="org.csstudio.opibuilder.samples.install"
               icon="icons/logentry-add-16.png"
               label="Install BOY Samples"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
