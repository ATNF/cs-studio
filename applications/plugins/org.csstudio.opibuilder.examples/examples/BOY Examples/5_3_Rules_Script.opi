  <widget typeId="org.csstudio.opibuilder.widgets.TextUpdate" version="1.0">
    <scripts />
    <height>20</height>
    <pv_name>loc://blinkingEnable</pv_name>
    <background_color>
      <color red="255" green="255" blue="255" />
    </background_color>
    <foreground_color>
      <color red="0" green="0" blue="0" />
    </foreground_color>
    <text>######</text>
    <font>
      <fontdata fontName="Arial" height="9" style="0" />
    </font>
    <rules>
      <rule name="Blinking" prop_id="background_color" out_exp="false">
        <exp bool_exp="pvStr2==&quot;Slow&quot; &amp;&amp; pv0==0">
          <value>
            <color red="255" green="0" blue="0" />
          </value>
        </exp>
        <exp bool_exp="pvStr2==&quot;Slow&quot; &amp;&amp; pv0==1">
          <value>
            <color red="255" green="255" blue="0" />
          </value>
        </exp>
        <exp bool_exp="pvStr2==&quot;Fast&quot; &amp;&amp; pv1==0">
          <value>
            <color red="128" green="0" blue="255" />
          </value>
        </exp>
        <pv trig="true">sim://ramp(0,1,1,0.5)</pv>
        <pv trig="true">sim://ramp(0,1,1,0.1)</pv>
        <pv trig="true">loc://blinkingEnable</pv>
      </rule>
    </rules>
    <actions hook="false" hook_all="false" />
    <y>431</y>
    <wrap_words>false</wrap_words>
    <tooltip>$(pv_name)
$(pv_value)</tooltip>
    <x>638</x>
  </widget>
