<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="CSS"
            id="org.csstudio">
      </category>
      <view
            allowMultiple="false"
            category="org.csstudio"
            class="org.csstudio.alarm.beast.ui.alarmtree.AlarmTreeView"
            icon="icons/alarmtree.gif"
            id="org.csstudio.alarm.beast.ui.alarmtree.View"
            name="%AlarmTree">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Global CSS menu actions"
            id="org.csstudio.alarm.beast.ui.alarmtree.cssmenu"
            label="CSS Action Set"
            visible="true">
         <action
               class="org.csstudio.alarm.beast.ui.alarmtree.ShowAlarmTreeAction"
               icon="icons/alarmtree.gif"
               id="org.csstudio.alarm.beast.ui.alarmtree.ShowAlarmTreeAction"
               label="%AlarmTree"
               menubarPath="css/alarm/"
               style="push"
               tooltip="%AlarmTree"/>
      </actionSet>
   </extension>
</plugin>
