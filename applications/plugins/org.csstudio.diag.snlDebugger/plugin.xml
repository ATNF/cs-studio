<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.csstudio"
            class="org.csstudio.diag.snlDebugger.ui.View"
            id="org.csstudio.diag.snlDebugger"
            name="snlDebugger"/>
      <category
            id="org.csstudio"
            name="CSS"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Global CSS menu actions"
            id="org.csstudio.diag.snlDebugger.actionSet"
            label="CSS Action Set"
            visible="true">
         <action
               class="org.csstudio.diag.snlDebugger.ui.ShowSnlDebugger"
               id="org.csstudio.diag.snlDebugger.action"
               label="snlDebugger"
               menubarPath="css/diag/"
               style="push"/>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.csstudio.platform.ui.css.applications.diag"
            class="org.csstudio.diag.snlDebugger.Preference.PageMain"
            id="org.csstudio.diag.snlDebugger.PreferencePage"
            name="snlDebugger"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.csstudio.diag.snlDebugger.Preference.PreferenceInitializer"/>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true"/>
      <toc
            file="toc.xml"
            primary="true"/>
   </extension>
   <extension
         point="org.csstudio.platform.authorizationId">
      <id
            description="Permission for the IOC tools."
            id="iocAccesses">
      </id>
   </extension>
