               icon="icons/addGroup.png"
               id="org.csstudio.opibuilder.widgets.creategroupAction"
               label="Create Group"
               menubarPath="group"
               tooltip="Create a group container which contains all the selected widgets.">
         </action>
         <visibility>
            <objectState
                  name="executionMode"
                  value="EDIT_MODE">
            </objectState>
         </visibility>
      </objectContribution>
