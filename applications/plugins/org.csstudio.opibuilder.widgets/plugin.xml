      </widget>
      <widget
            category="Monitors"
            description="A meterwhich could display numeric value."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.XMeterEditPart"
            icon="icons/XMeter.png"
            model_class="org.csstudio.opibuilder.widgets.model.XMeterModel"
            name="Meter"
            typeId="org.csstudio.opibuilder.widgets.meter">
      </widget>
      <widget
            category="Monitors"
            description="An XY graph which can display the trends of multiple PVs."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.XYGraphEditPart"
            icon="icons/XYGraph.gif"
            model_class="org.csstudio.opibuilder.widgets.model.XYGraphModel"
            name="XY Graph"
            typeId="org.csstudio.opibuilder.widgets.xyGraph">
      </widget>
      <widget
            category="Controls"
            description="A scaled slider widget which can read/write a PV"
            editpart_class="org.csstudio.opibuilder.widgets.editparts.ScaledSliderEditPart"
