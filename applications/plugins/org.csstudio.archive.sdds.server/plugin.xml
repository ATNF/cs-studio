<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="SddsServerApplication"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.csstudio.archive.sdds.server.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.csstudio.platform.managementCommands">
      <command
            class="org.csstudio.archive.sdds.server.management.Stop"
            id="org.csstudio.archive.sdds.server.Stop"
            label="Stop">
      </command>
      <command
            class="org.csstudio.archive.sdds.server.management.Restart"
            id="org.csstudio.archive.sdds.server.Restart"
            label="Restart">
      </command>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.csstudio.archive.sdds.server.internal.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         id="SDDSServerProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.archive.sdds.server.SDDS-ServerApplication"
            name="SDDS-Server">
         <property
               name="appName"
               value="SDDS-Server">
         </property>
      </product>
   </extension>

</plugin>
