<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.csstudio.alarm.beast.client.AlarmTreeItem"
            class="org.csstudio.beast.ui.adapters.LogbookBuilderFactory">
         <adapter
               type="org.csstudio.logbook.LogEntryBuilder">
         </adapter>
      </factory>
      <factory
            adaptableType="org.csstudio.alarm.beast.client.AlarmTreeLeaf"
            class="org.csstudio.beast.ui.adapters.LogbookBuilderFactory">
         <adapter
               type="org.csstudio.logbook.LogEntryBuilder">
         </adapter>
      </factory>
   </extension>

</plugin>
