<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="VoicemailConnector"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="org.csstudio.ams.connector.voicemail.VoicemailConnectorStart"/>
      </application>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.csstudio.ams.connector.voicemail.internal.PreferencesInitializer"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.csstudio.ams.internal.pages.AmsPreferencePage"
            class="org.csstudio.ams.connector.voicemail.internal.pages.VoicemailPreferencePage"
            id="org.csstudio.ams.connector.voicemail.VoicemailPreferencePage"
            name="Connector Voice Mail"/>
   </extension>
   <extension
         point="org.csstudio.platform.libs.xmpp.loginHandlers">
      <loginHandler
            class="org.csstudio.ams.connector.voicemail.xmpp.VoicemailCallbackHandler"
            id="org.csstudio.ams.connector.voicemail.VoicemailLoginHandler"
            name="VoicemailLoginHandler">
      </loginHandler>
   </extension>
   <extension
         point="org.csstudio.platform.libs.dcf.action">
      <parametrizedAction
            class="org.csstudio.ams.connector.voicemail.actions.VoicemalConnectorStopAction"
            id="org.csstudio.ams.connector.voicemail.VoicemalConnectorStopAction"
            name="Stop"
            parameters="Password">
      </parametrizedAction>
   </extension>
   <extension
         id="VoicemailConnectorProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.ams.connector.voicemail.VoicemailConnector"
            name="VoicemailConnector">
         <property
               name="appName"
               value="VoicemailConnector">
         </property>
      </product>
   </extension>

</plugin>
