            typeId="org.csstudio.opibuilder.widgets.TextUpdate">
      </widget>
      <widget
            category="Controls"
            description="Write value to a pv by inputing text in this widget."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.TextInputEditpart"
            icon="icons/text.gif"
            model_class="org.csstudio.opibuilder.widgets.model.TextInputModel"
            name="Text Input"
            typeId="org.csstudio.opibuilder.widgets.TextInput">
      </widget>
      <widget
            category="Graphics"
            description="Create a ellipse widget"
            editpart_class="org.csstudio.opibuilder.widgets.editparts.EllipseEditpart"
            icon="icons/ellipse.gif"
            model_class="org.csstudio.opibuilder.widgets.model.EllipseModel"
            name="Ellipse"
            typeId="org.csstudio.opibuilder.widgets.Ellipse">
      </widget>
      <widget
            category="Monitors"
            description="A thermeter which could display numeric value."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.ThermometerEditPart"
            icon="icons/Thermo.gif"
            model_class="org.csstudio.opibuilder.widgets.model.ThermometerModel"
            name="Thermometer"
            typeId="org.csstudio.opibuilder.widgets.thermometer">
      </widget>
      <widget
            category="Monitors"
            description="A gauge which could display numeric value."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.GaugeEditPart"
            icon="icons/gauge.gif"
            model_class="org.csstudio.opibuilder.widgets.model.GaugeModel"
            name="Gauge"
            typeId="org.csstudio.opibuilder.widgets.gauge">
      </widget>
      <widget
            category="Monitors"
            description="A tank which could display numeric value."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.TankEditPart"
