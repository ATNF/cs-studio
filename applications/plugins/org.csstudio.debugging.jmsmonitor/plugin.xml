<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.csstudio"
            name="CSS">
      </category>
      <view
            category="org.csstudio"
            class="org.csstudio.debugging.jmsmonitor.JMSMonitorView"
            icon="icons/jmsmonitor.gif"
            id="org.csstudio.debugging.jmsmonitor.view"
            name="%JMSMonitor"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Global CSS menu actions"
            id="org.csstudio.debugging.jmsmonitor.cssmenu"
            label="CSS Action Set"
            visible="true">
         <action
               class="org.csstudio.debugging.jmsmonitor.ShowJMSMonitorAction"
               icon="icons/jmsmonitor.gif"
               id="org.csstudio.debugging.jmsmonitor.ShowJMSMonitorAction"
               label="%JMSMonitor"
               menubarPath="css/debugging/"
               style="push"/>
      </actionSet>
   </extension>

</plugin>
