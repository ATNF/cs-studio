            name="LED"
            typeId="org.csstudio.opibuilder.widgets.LED">
      </widget>
      <widget
            category="Controls"
            description="A boolean switch which can write a single bit of a PV or write 0/1 to a PV when bit property is -1."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.BoolSwitchEditPart"
            icon="icons/BoolSwitch.gif"
            model_class="org.csstudio.opibuilder.widgets.model.BoolSwitchModel"
            name="Boolean Switch"
            typeId="org.csstudio.opibuilder.widgets.BoolSwitch">
      </widget>
      <widget
            category="Controls"
            description="A boolean button which can write a single bit of a PV or write 0/1 to a PV when bit property is -1."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.BoolButtonEditPart"
            icon="icons/BoolButton.gif"
            model_class="org.csstudio.opibuilder.widgets.model.BoolButtonModel"
            name="Boolean Button"
            typeId="org.csstudio.opibuilder.widgets.BoolButton">
      </widget>
      <widget
            category="Others"
            description="A container widget which can group other widgets."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.GroupingContainerEditPart"
            icon="icons/groupContainer.png"
            model_class="org.csstudio.opibuilder.widgets.model.GroupingContainerModel"
            name="Grouping Container"
            typeId="org.csstudio.opibuilder.widgets.groupingContainer">
      </widget>
      <widget
            category="Others"
            description="A container widget which can load another OPI file."
            editpart_class="org.csstudio.opibuilder.widgets.editparts.LinkingContainerEditpart"
            icon="icons/linkingcontainer.png"
            model_class="org.csstudio.opibuilder.widgets.model.LinkingContainerModel"
            name="Linking Container"
            typeId="org.csstudio.opibuilder.widgets.linkingContainer">
      </widget>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.csstudio.opibuilder.widgets.createGroup"
            objectClass="org.csstudio.opibuilder.editparts.AbstractBaseEditPart">
         <action
               class="org.csstudio.opibuilder.widgets.editparts.CreateGroupAction"
               enablesFor="+"
               icon="icons/addGroup.png"
               id="org.csstudio.opibuilder.widgets.creategroupAction"
               label="Create Group"
               menubarPath="group"
               tooltip="Create a group container which contains all the selected widgets.">
         </action>
         <visibility>
            <objectState
                  name="executionMode"
                  value="EDIT_MODE">
            </objectState>
         </visibility>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.csstudio.opibuilder.widgets.removeGroup"
            objectClass="org.csstudio.opibuilder.widgets.editparts.GroupingContainerEditPart">
         <action
               class="org.csstudio.opibuilder.widgets.editparts.RemoveGroupAction"
               enablesFor="1"
               icon="icons/removeGroup.png"
               id="org.csstudio.opibuilder.widgets.removeGroupAction"
               label="Remove Group"
               menubarPath="group"
               tooltip="Remove the group container.">
         </action>
         <action
               class="org.csstudio.opibuilder.widgets.editparts.PerformAutoSizeOnGroupAction"
               enablesFor="1"
               icon="icons/autoSizeGroup.png"
               id="org.csstudio.opibuilder.widgets.performAutoSize"
               label="Perform AutoSize"
               menubarPath="group"
               tooltip="Perform AutoSize on the selected Group Container.">
         </action>
         <visibility>
            <objectState
                  name="executionMode"
                  value="EDIT_MODE">
            </objectState>
         </visibility>
      </objectContribution>
   </extension>
   <extension
         point="org.csstudio.opibuilder.graphicalFeedbackFactory">
      <graphicalFeedbackFactory
            class="org.csstudio.opibuilder.widgets.feedback.PolyLineFeedbackFactory"
            typeId="org.csstudio.opibuilder.widgets.polyline">
      </graphicalFeedbackFactory>
      <graphicalFeedbackFactory
            class="org.csstudio.opibuilder.widgets.feedback.PolygonFeedbackFactory"
            typeId="org.csstudio.opibuilder.widgets.polygon">
      </graphicalFeedbackFactory>
   </extension>

</plugin>
