      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.csstudio.utility.pvmanager.ui"/>
      <import plugin="org.csstudio.csdata" version="0.0.1" match="greaterOrEqual"/>
      <import plugin="org.csstudio.ui.util" version="0.0.1" match="greaterOrEqual"/>
