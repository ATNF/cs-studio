  <widget typeId="org.csstudio.opibuilder.widgets.Label" version="1.0">
    <scripts />
    <height>20</height>
    <name>Label_8</name>
    <transparent>true</transparent>
    <background_color>
      <color red="255" green="255" blue="255" />
    </background_color>
    <foreground_color>
      <color red="0" green="0" blue="0" />
    </foreground_color>
    <text>result</text>
    <font>
      <fontdata fontName="Arial" height="12" style="1" />
    </font>
    <width>150</width>
    <rules>
      <rule name="Text Rule" prop_id="text" out_exp="false">
        <exp bool_exp="pv0==pv1">
          <value>Draw!</value>
        </exp>
        <exp bool_exp="pv0&gt;pv1">
          <value>Left Win!</value>
        </exp>
        <exp bool_exp="pv0&lt;pv1">
          <value>Right Win!</value>
        </exp>
        <pv trig="true">loc://knob0</pv>
        <pv trig="true">loc://knob1</pv>
      </rule>
      <rule name="Color Rule" prop_id="foreground_color" out_exp="false">
        <exp bool_exp="pv0==pv1">
          <value>
            <color red="0" green="255" blue="0" />
          </value>
        </exp>
        <exp bool_exp="pv0&gt;pv1">
          <value>
            <color red="255" green="0" blue="0" />
          </value>
        </exp>
        <exp bool_exp="pv0&lt;pv1">
          <value>
            <color red="0" green="0" blue="255" />
          </value>
        </exp>
        <pv trig="true">loc://knob0</pv>
        <pv trig="true">loc://knob1</pv>
      </rule>
    </rules>
    <actions hook="false" hook_all="false" />
    <y>128</y>
    <x>512</x>
  </widget>
