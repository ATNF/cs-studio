<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.csstudio"
            name="CSS"/>
      <view
            category="org.csstudio"
            class="org.csstudio.ams.monitor.views.AMSMonitorView"
            icon="icons/ams.gif"
            id="org.csstudio.ams.monitor.views.AMSMonitorView"
            name="AMS Monitor"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.csstudio.ams.monitor.actionSet"
            label="amsMonitor Actionset"
            visible="true">
         <action
               class="org.csstudio.ams.monitor.ShowPluginAction"
               icon="icons/ams.gif"
               id="org.csstudio.ams.monitor.action1"
               label="AMS Monitor"
               menubarPath="css/diag/"
               style="push"/>
      </actionSet>
   </extension>

</plugin>
