<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
<extension
         point="org.eclipse.ui.views">
      <category
            id="org.csstudio.nams.newconfigurator"
            name="AMS">
      </category>
      <view
            allowMultiple="false"
            category="org.csstudio.nams.newconfigurator"
            class="org.csstudio.nams.configurator.views.AlarmbearbeiterView"
            fastViewWidthRatio=".5f"
            id="org.csstudio.nams.configurator.alarmbearbeiter"
            name="Alarmbearbeiter">
      </view>
      <view
            allowMultiple="false"
            category="org.csstudio.nams.newconfigurator"
            class="org.csstudio.nams.configurator.views.AlarmbearbeitergruppenView"
            fastViewWidthRatio=".5f"
            id="org.csstudio.nams.configurator.alarmbearbeitergruppen"
            name="Alarmbearbeitergruppen">
      </view>
      <view
            allowMultiple="false"
            category="org.csstudio.nams.newconfigurator"
            class="org.csstudio.nams.configurator.views.AlarmtopicView"
            fastViewWidthRatio=".5f"
            id="org.csstudio.nams.configurator.alarmtopic"
            name="Alarmtopic">
      </view>
      <view
            allowMultiple="false"
            category="org.csstudio.nams.newconfigurator"
            class="org.csstudio.nams.configurator.views.FilterView"
            fastViewWidthRatio=".5f"
            id="org.csstudio.nams.configurator.filter"
            name="Filter">
      </view>
      <view
            allowMultiple="false"
            category="org.csstudio.nams.newconfigurator"
            class="org.csstudio.nams.configurator.views.FilterbedingungView"
            fastViewWidthRatio="0.5f"
            id="org.csstudio.nams.configurator.filterbedingung"
            name="Filterbedingung">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.csstudio.nams.configurator.editor.ConfigurationEditor"
            icon="icons/ams.gif"
            id="org.csstudio.nams.configurator.ConfigurationEditor"
            name="Configuration Editor">
      </editor>
      <editor
            class="org.csstudio.nams.configurator.editor.AlarmbearbeiterEditor"
            icon="icons/ams.gif"
            id="org.csstudio.nams.configurator.editor.AlarmbearbeiterEditor"
            name="Alarmbearbeiter Editor">
      </editor>
      <editor
            class="org.csstudio.nams.configurator.editor.AlarmbearbeitergruppenEditor"
            icon="icons/ams.gif"
            id="org.csstudio.nams.configurator.editor.AlarmbearbeitergruppenEditor"
            name="Alarmbearbeitergruppen Editor">
      </editor>
      <editor
            class="org.csstudio.nams.configurator.editor.AlarmtopicEditor"
            icon="icons/ams.gif"
            id="org.csstudio.nams.configurator.editor.AlarmtopicEditor"
            name="Alarmtopic Editor">
      </editor>
      <editor
            class="org.csstudio.nams.configurator.editor.FilterEditor"
            icon="icons/ams.gif"
            id="org.csstudio.nams.configurator.editor.FilterEditor"
            name="Filter Editor">
      </editor>
      <editor
            class="org.csstudio.nams.configurator.editor.FilterbedingungEditor"
            icon="icons/ams.gif"
            id="org.csstudio.nams.configurator.editor.FilterbedingungEditor"
            name="FilterbedingungEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.csstudio.nams.newconfigurator.perspective">
         <view
               closeable="false"
               id="org.csstudio.nams.configurator.alarmbearbeiter"
               minimized="false"
               moveable="true"
               ratio="0.4f"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.csstudio.nams.configurator.alarmbearbeitergruppen"
               minimized="false"
               moveable="true"
               ratio="0.4f"
               relationship="top"
               relative="org.csstudio.nams.configurator.alarmbearbeiter"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.csstudio.nams.configurator.filterbedingung"
               minimized="false"
               moveable="true"
               ratio="0.4f"
               relationship="stack"
               relative="org.csstudio.nams.configurator.alarmbearbeiter"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.csstudio.nams.configurator.alarmtopic"
               minimized="false"
               moveable="true"
               ratio="0.4f"
               relationship="stack"
               relative="org.csstudio.nams.configurator.filterbedingung"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.csstudio.nams.configurator.filter"
               minimized="false"
               moveable="true"
               ratio="0.4f"
               relationship="stack"
               relative="org.csstudio.nams.configurator.alarmbearbeitergruppen"
               showTitle="true"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.csstudio.nams.configurator.PerspectiveFactory"
            fixed="true"
            id="org.csstudio.nams.newconfigurator.perspective"
            name="AMS perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.csstudio.nams.configurator.actions.CreateNewEmptyEditor"
            id="newEditor.command"
            name="Neu...">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.csstudio.nams.configurator.alarmbearbeiter">
         <command
               commandId="newEditor.command"
               id="newEditor.command.contribution">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.csstudio.nams.configurator.actions.DeleteConfiguration"
            id="deleteConfiguration.command"
            name="Delete">
         <commandParameter
               id="deleteConfiguration.command.target"
               name="target"
               values="org.csstudio.nams.configurator.beans.AlarmbearbeiterBean">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.csstudio.nams.configurator.alarmbearbeiter">
         <command
               commandId="deleteConfiguration.command"
               id="deleteConfiguration.command.contribution">
         </command>
      </menuContribution>
   </extension>
w

</plugin>
