<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="AmsJMSConnectorId"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.csstudio.ams.connector.jms.JMSConnectorStart">
         </run>
      </application>
   </extension>
   <extension
         id="AmsJmsConnectorProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.ams.connector.jms.AmsJMSConnectorId"
            name="AmsJmsConnector">
         <property
               name="appName"
               value="AmsJmsConnector">
         </property>
      </product>
   </extension>
   <extension
         point="org.csstudio.platform.managementCommands">
      <command
            class="org.csstudio.ams.connector.jms.management.Stop"
            id="org.csstudio.ams.connector.jms.management.Stop"
            label="Stop">
         <stringParameter
               id="Password"
               label="Password">
         </stringParameter>
      </command>
      <command
            class="org.csstudio.ams.connector.jms.management.Restart"
            id="org.csstudio.ams.connector.jms.management.Restart"
            label="Restart">
         <stringParameter
               id="Password"
               label="Password">
         </stringParameter>
      </command>
   </extension>

</plugin>
